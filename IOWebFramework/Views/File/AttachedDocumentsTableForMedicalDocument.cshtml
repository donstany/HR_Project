@using IOWebFramework.Core.Helper.GlobalConstants

@model IOWebFramework.Core.Models.Attachments.AttachedDocumentsTableForDocumentsViewModel

@{
//Layout = "~/Areas/Internal/Views/Shared/_MasterLayout.cshtml";

//ViewData["Title"] = "Прикачени файлове";
}
<div class="row">
    <div class="col-lg-12">
        <div class="box box-primary">
            <div class="box-body">
                <h4>
                    @if (Model.AttachedDocumentsTableViewModel.DocumentType == DocumentTypesConstant.Diploma)
                    {
                        <span>Диплома - №</span>
                    }
                    <span>@Model.DocumentNumber</span>
                </h4>
                <hr />
            </div>
            </div>
        </div>
    </div>

    <div class="row">
        <div class="col-lg-12">
            @await Html.PartialAsync("_AttachedDocumentsTablePartial", Model.AttachedDocumentsTableViewModel)
        </div>
    </div>


